@article{arguelloComparisonSegmentationTools2019,
  title = {Comparison of Segmentation Tools for Structural Analysis of Bone Tissues by Finite Elements},
  author = {Argüello, D and Sánchez Acevedo, H G and González-Estrada, O A},
  date = {2019-11-01},
  journaltitle = {Journal of Physics: Conference Series},
  shortjournal = {J. Phys.: Conf. Ser.},
  volume = {1386},
  number = {1},
