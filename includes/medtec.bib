@article{vicarelliCreationNationalHealth2019,
  title = {The Creation of the {{National Health System}} in {{Italy}} (1961-1978)},
  author = {Vicarelli, Giovanna},
  date = {2019-05-10},
  journaltitle = {Dynamis},
  shortjournal = {dynamis},
